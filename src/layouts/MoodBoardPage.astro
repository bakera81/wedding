---
import Layout from "./Layout.astro";
import CeremonyDressCode from '../components/CeremonyDressCode.astro'
import MischiefDressCode from '../components/MischiefDressCode.astro'

interface Props {
  footer: string;
}

const { footer }  = Astro.props;
---

<Layout footer={footer} title="Dress Code">
  <MischiefDressCode footer={footer}/>
  {footer === '/mischief' ? (<></>) : (<CeremonyDressCode />)}
</Layout