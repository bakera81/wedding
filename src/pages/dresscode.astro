import img3 from "../images/dresscode/dancing.jpg";
import img4 from "../images/dresscode/deer.jpeg";
import img5 from "../images/dresscode/desert_cult.jpg";
import img6 from "../images/dresscode/goat_wizard.jpeg";
import img7 from "../images/dresscode/infinite_cult.jpeg";
import img8 from "../images/dresscode/jumpsuit.jpeg";
import img9 from "../images/dresscode/midsommar_children.jpeg";
import img10 from "../images/dresscode/midsommar.jpeg";
